<!--Formato de un archivo de nivel-->
<level>

   
  <!--El terrain es el unico item obligatorio del level.-->
  <terrain
   heightmap="pathHeightmap"
   texture="pathTextura"
   scaleXZ="float"
   scaleY="float"
    ></terrain>
  
  <!-- class es opcional, si no se pone nada, usa clase Commando -->
  <commando class="tipoComando">(X,Y,Z)</commando>
 
  <!-- class es obligatorio. Por ahora la unica disponible es "soldier"-->
  <enemy class="soldier">
    <!--La clase soldier tiene de 1 a N waitpoints-->
    <waitpoint>(X,Y,Z)</waitpoint>
    <waitpoint>(X,Y,Z)</waitpoint>
    <waitpoint>(X,Y,Z)</waitpoint>
  </enemy>
  
  
  <!--class es opcional. La default es levelObject. Disponibles: "tree"-->
  <!--mesh solo lo pide levelObject-->
  <!--scale es opcional, el default es (1,1,1)-->
  <levelObject mesh="pathDelTgcScene.xml" scale="(X,Y,Z)">(X,Y,Z)</levelObject>
  
</level>  