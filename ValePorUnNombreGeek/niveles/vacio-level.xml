<!--Formato de un archivo de nivel-->
<level>

   
  <!--El terrain es el unico item obligatorio del level.-->
  <terrain
   heightmap="pathHeightmap"
   texture="pathTextura"
   scaleXZ="float"
   scaleY="float"
    ></terrain>
  
  <!-- class es opcional, si no se pone nada, usa clase Commando -->
  <commando class="tipoComando">(X,Z)</commando>
 
  <!-- class es obligatorio. Por ahora la unica disponible es "soldier"-->
  <enemy class="soldier">
    <!--La clase soldier tiene de 1 a N waitpoints-->
    <waitpoint>(X,Z)</waitpoint>
    <waitpoint>(X,Z)</waitpoint>
    <waitpoint>(X,Z)</waitpoint>
  </enemy>
  
  
  <!--class es opcional. La default es meshObject. Disponibles: "tree", "meshObject"-->
  <!--scale es opcional, default (1,1,1)-->
  <!--rotation es opcional, default (0,0,0)-->
  <levelObject mesh="pathDelTgcScene.xml" scale="(sX,sY,sZ)" rotation="(radian, radian, radian)">(X,Z)</levelObject>
  <levelObject scale="(sX,sY,sZ)" rotation="(radian, radian, radian)" class="tree">(X,Z)</levelObject>

</level>  